{
	"manifest_version" : 2,
	"name" : "Mascot",
	"version" : "1.0",

	"content_scripts" : [{
		"matches" : ["*://*.cunyfirst.cuny.edu/*"],
		"css" : ["css/global.css"],
		"js" : ["js/global.js", "js/navigation.js", "js/notifications-center.js"]
		}, 
		{
			"matches" : ["*://*.cunyfirst.cuny.edu/psp/cnyepprd/EMPLOYEE/EMPL/h/*"],
			"css": ["css/landing.css"],
			"js" : ["js/landingUI.js"]
		},
		{
			"matches" : ["https://home.cunyfirst.cuny.edu/oam/Portal_Login1.html"],
			"css" : ["css/login.css"],
			"js" : ["js/loginUI.js"]
		}
	],
	"web_accessible_resources": [
	/* Images */
   	"img/logo_wide_small.png", 

    /* CSS Files */
    "css/global.css",
    "css/landing.css",
    "css/login.css",
    "css/nav.css"
  ]
}